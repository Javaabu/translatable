{
    "name": "javaabu/translatable",
    "description": "Adds multi-lingual to Laravel models",
    "homepage": "https://github.com/Javaabu/translatable",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "translatable"
    ],
    "authors": [
        {
            "name": "Xylam",
            "email": "jailam@javaabu.com",
            "role": "Developer"
        },
        {
            "name": "FlameXode",
            "email": "hi@flamexo.de",
            "role": "Developer"
        },
        {
            "name": "Athphane",
            "email": "athphane@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "javaabu/helpers": "^1.61"
    },
    "suggest": {
        "javaabu/forms": "For the little buttons and table cells that are provided."
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^9.5 || ^10.5 || ^11.5"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Translatable\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Translatable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Translatable\\TranslatableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
